{
    "name": "alan6k8/sorted-linked-list",
    "type": "project",
    "description": "Just an example implementation of library",
    "homepage": "https://github.com/alan6k8/sorted-linked-list",
    "license": "GPL-3.0-or-later",
    "authors": [
